{
  "name": "nodejs-template",
  "version": "0.1.0",
  "description": "Template for the development of Node.js APIs",
  "private": true,
  "apidoc": {
    "title": "Node.js Template",
    "sampleUrl": "http://localhost:4004/v1",
    "order": [
      "get",
      "post",
      "put",
      "delete"
    ]
  },
  "scripts": {
    "start": "tsc && node ./bin/www"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "debug": "4.1.0",
    "dotenv": "6.1.0",
    "express": "4.16.3",
    "mysql": "2.16.0",
    "log": "1.4.0",
    "morgan": "1.9.1",
    "multer": "1.4.0",
    "request": "2.88.0",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "@types/node": "10.11.6",
    "@types/request": "2.47.1",
    "module-alias": "2.1.0",
    "typescript": "3.1.2"
  },
  "_moduleAliases": {
    "@http": "dist/http/",
    "@controllers": "dist/http/controllers",
    "@repositories": "dist/src/repositories",
    "@models": "dist/src/models",
    "@sql": "dist/src/sources/sql"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "routes",
      "docs",
      "app.ts"
    ],
    "ext": "ts"
  }
}